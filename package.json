{
  "name": "typespec-moonbit",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "devDependencies": {
    "@azure-tools/azure-http-specs": "^0.1.0-alpha.36",
    "@azure-tools/typespec-autorest": "~0.64.0",
    "@azure-tools/typespec-azure-core": "~0.64.0",
    "@azure-tools/typespec-azure-resource-manager": "~0.64.0",
    "@azure-tools/typespec-client-generator-core": "~0.64.1",
    "@typespec/compiler": "^1.8.0",
    "@typespec/http": "^1.8.0",
    "@typespec/http-specs": "0.1.0-alpha.30",
    "@typespec/rest": "~0.78.0",
    "@typespec/spector": "0.1.0-alpha.22",
    "@typespec/versioning": "~0.78.0",
    "@typespec/xml": "~0.78.0"
  },
  "scripts": {
    "build:client": "node .scripts/build_emitters.js --client",
    "build:server": "node .scripts/build_emitters.js --server",
    "build:emitters": "node .scripts/build_emitters.js --all",
    "prepare": "node .scripts/build_emitters.js --all",
    "spector": "node .scripts/spector.js",
    "gen:client-test": "node .scripts/gen_client_test.js",
    "gen:server-test": "node .scripts/gen_server_test.js",
    "test:server:run": "moon test -C tests -p kkkiio/typespec-tests/server",
    "test:server": "npm run gen:server-test && npm run test:server:run",
    "test:client": "npm run gen:client-test && moon test -C tests --target native -p kkkiio/typespec-tests/client/tests"
  }
}