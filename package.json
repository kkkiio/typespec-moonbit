{
  "name": "typespec-moonbit",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "main": "./index.js",
  "exports": {
    ".": "./index.js"
  },
  "tspMain": "./index.js",
  "files": [
    "index.js",
    "README.md",
    "LICENSE",
    "typespec-moonbit/_build/js/release/build/**"
  ],
  "devDependencies": {
    "@azure-tools/azure-http-specs": "^0.1.0-alpha.36",
    "@azure-tools/typespec-autorest": "~0.64.0",
    "@azure-tools/typespec-azure-core": "~0.64.0",
    "@azure-tools/typespec-azure-resource-manager": "~0.64.0",
    "@azure-tools/typespec-client-generator-core": "~0.64.1",
    "@typespec/compiler": "^1.8.0",
    "@typespec/http": "^1.8.0",
    "@typespec/http-specs": "0.1.0-alpha.30",
    "@typespec/rest": "~0.78.0",
    "@typespec/spector": "0.1.0-alpha.22",
    "@typespec/versioning": "~0.78.0",
    "@typespec/xml": "~0.78.0"
  },
  "scripts": {
    "build:js": "moon build -C typespec-moonbit --target js --release",
    "prepare": "npm run build:js",
    "spector": "node .scripts/spector.js",
    "gen:client-test": "node .scripts/tspcompile.js",
    "gen:server-test": "node .scripts/emit_e2e_server.js",
    "test:server:run": "moon test -C typespec-moonbit-tests -p kkkiio/typespec-moonbit-tests/server",
    "test:server": "npm run gen:server-test && npm run test:server:run",
    "test:client": "npm run gen:client-test -- --filter=spector_routes --limit=1 && moon test -C typespec-moonbit-tests --target native -p kkkiio/typespec-moonbit-tests/client/tests"
  }
}
